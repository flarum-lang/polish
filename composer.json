{
	"name": "flarum-lang/polish",
	"type": "flarum-extension",
	"description": "Polish language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/polish",
	"keywords": [
		"locale",
		"flarum",
		"polish-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Robert Korulczyk",
			"email": "robert@korulczyk.pl",
			"homepage": "https://rob006.net/"
		}
	],
	"support": {
		"issues": "https://github.com/flarum-lang/polish/issues",
		"source": "https://github.com/flarum-lang/polish"
	},
	"require": {
		"flarum/core": "^1.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"flarum-extension": {
			"title": "Polish",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "pl",
			"title": "Polski"
		},
		"extiverse": {
			"discuss": "https://discuss.flarum.org/d/18134"
		}
	}
}
